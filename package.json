{
  "name": "sankey",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile-js",
    "build-full": "npm-run-all --parallel compile-js compile-scss compile-css",
    "compile-js": "rollup -cw",
    "compile-scss": "sass src/css:docs/css --no-source-map --watch",
    "compile-css": "postcss docs/css/index.css --no-map -o docs/css/index.css -c postcss.config.js --watch",
    "dev-listglobals": "npm list -g --depth 0",
    "dev-serve": "browser-sync docs -w",
    "eslint": "eslint -c .eslintrc.json --ext .ts --ext .js ./src",
    "http": "http-server ./docs -a localhost -p 5000 -o",
    "https": "http-server ./docs -a localhost -p 5000 -o --ssl --key private/key.pem --cert private/cert.pem",
    "imagemin": "imagemin src/images/* --out-dir=docs/images",
    "start": "pm2 start npm -- start-app -w",
    "start-app": "npm run http",
    "test": "npm run http",
    "test-compile": "jest --config jestconfig.json",
    "update-all": "npm run update-jszip",
    "update-jszip": "ncp node_modules/jszip/dist docs/lib/jszip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibuckner/sankey.git"
  },
  "author": "NEL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibuckner/sankey/issues"
  },
  "homepage": "https://github.com/ibuckner/sankey#readme",
  "dependencies": {
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-transition": "^1.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/d3": "^5.7.2",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "autoprefixer": "^9.7.6",
    "clean-css": "^4.2.3",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^24.0.0",
    "jest": "^25.5.0",
    "jsdoc": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-sorting": "^5.0.1",
    "rollup": "^2.7.3",
    "rollup-plugin-typescript2": "^0.27.0",
    "sass": "^1.26.5",
    "stylelint": "^13.3.3",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
