{
  "name": "@buckneri/sankey",
  "version": "0.1.1",
  "description": "Just a sankey visualisation",
  "main": "./dist/sankey.js",
  "module": "./dist/sankey.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run compile-js & npm run m-rmdir",
    "compile-js": "rollup -c",
    "compile-scss": "sass src/css:docs/css --no-source-map",
    "compile-css": "postcss docs/css/sankey.css --no-map -o docs/css/sankey.css -c postcss.config.js",
    "eslint": "eslint -c .eslintrc.json --ext .ts --ext .js ./src",
    "list-globals": "npm list -g --depth 0",
    "make": "npm run compile-scss & npm run compile-css & npm run m-typedef & npm run m-rmdir & npm run m-css",
    "m-css": "cpy docs/css/sankey.css dist",
    "m-typedef": "move-file dist/src/index.d.ts dist/index.d.ts",
    "m-rmdir": "rimraf dist/src/ docs/lib/src",
    "test": "http-server ./docs -a localhost -p 5000 -o",
    "test-upload": "npm publish --dry-run --access public",
    "upload": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibuckner/sankey.git"
  },
  "author": "Ivor Buckner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibuckner/sankey/issues"
  },
  "homepage": "https://github.com/ibuckner/sankey#readme",
  "dependencies": {
    "@buckneri/spline": "^0.2.3",
    "d3-drag": "^1.2.5",
    "d3-format": "^1.4.4",
    "d3-path": "^1.0.9",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-transition": "^1.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/d3": "^5.7.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "autoprefixer": "^9.7.6",
    "clean-css": "^4.2.3",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^24.0.2",
    "jsdoc": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-sorting": "^5.0.1",
    "rollup": "^2.7.6",
    "rollup-plugin-typescript2": "^0.27.0",
    "sass": "^1.26.5",
    "stylelint": "^13.3.3",
    "typescript": "^3.8.3"
  }
}
